<h2>Green Category</h2>

<ul>
  <% colors.forEach((oneColor) => {
     if (oneColor.category === ‘green’){ %>

       <li>

         <div style=“width:60px;height:40px;background-color:<%= oneColor.hex %>; border: 1px solid black;“></div>
         <%= oneColor.keyword %>
         <%= oneColor.hex %>


       </li>

   <%  };
    }) %>
</ul>
